@model HomePageViewModel

@{
    ViewData["Title"] = "Home";
}

<div class="flex flex-col items-center py-4 gap-y-10">
    @await Html.PartialAsync("Partials/_CarouselSection")
    @await Html.PartialAsync("Partials/_BrandsSection")
    @await Html.PartialAsync("Partials/_CategoriesSection", Model.Categories)
    @await Html.PartialAsync("Partials/_FeaturedProductsSection", Model.Products)
</div>
