      ],
      "TokenEndpointUris": [
        "connect/token"
      ],
      "UserInfoEndpointUris": [
        "connect/userinfo"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
